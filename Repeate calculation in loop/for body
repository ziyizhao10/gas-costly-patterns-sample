//unoptimized code
pragma solidity 0.7.0;

contract C {
    uint a = 1;
    uint b = 2;
    function p10() public  {
        uint sum = 0;
        for(uint i = 0; i <= x; i++) {
            sum = sum + a * b;
        }
    }
}



//optimized code
pragma solidity 0.7.0;

contract C {
    uint a = 1;
    uint b = 2;
    function p10() public  {
        uint sum = 0;
        for(uint i = 0; i <= x; i++) {
            sum = sum + c;
        }
    }
}
