//unoptimized code
pragma solidity 0.7.0;

contract B {
    function p8(uint x) public returns(uint){
        if(x<=0){
            if(x<1){
                return x;
            }
        }
    }
}



//optimized code
pragma solidity 0.7.0;

contract B {
    function p8(uint x) public returns(uint){
        if(x<=0){
            return x;
        }
    }
}
