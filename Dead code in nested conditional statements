//unoptimized code
pragma solidity 0.7.0;

contract B {
    function p7(uint x) public returns(uint){
        if(x<1){
            if(x>2){
                return x;
            }
        }
    }
}



//optimized code
pragma solidity ^ 0.7.0;

contract B {
    function p7(uint x) public returns(uint){
        if(x<1){
            return x;
        }
    }
}
